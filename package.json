{
  "name": "latinopool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "commit": "git-cz"
  },
  "dependencies": {
    "apisauce": "^2.1.1",
    "next": "10.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.16.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/react": "17.0.9",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.1.4",
    "typescript": "4.3.2"
  },
    "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
    "pre-commit": {
    "run": [
      "pretty-quick",
      "lint:fix",
      "lint",
      "git-add"
    ]
  },
    "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

